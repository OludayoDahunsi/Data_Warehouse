Denormalization Table Schema
CREATE TABLE DiscountMediaCampaign_Report (
    pid int PRIMARY KEY,
    DiscountID INT,
    DiscountAmount DOUBLE,
    RequiredSpendAmount INT,
    DiscountDescription TEXT,
    CampaignID INT,
    DiscountType TEXT,
    PersonID INT,
    FirstName TEXT,
    LastName TEXT,
    DOB TIMESTAMP,
    RoleStatus TEXT,
    RoleID INT,
    AddressID INT,
    StartDate TIMESTAMP,
    EndDate TIMESTAMP,
    Budget INT,
    DemographicID INT,
    CampaignType TEXT,
    CampaignNotes TEXT,
    AgeLowerLimit INT,
    AgeUpperLimit INT,
    TargetCity TEXT,
    InteractionFrequencyType INT
);








