CREATE TABLE IF NOT EXISTS mcdonald.address (
    pid int PRIMARY KEY,
    AddressID INT,
    Country TEXT,
    State TEXT,
    City TEXT,
    ZipCode INT,
    StreetAddress TEXT,
    AddressType TEXT
);

SELECT * FROM mcdonald.address;

